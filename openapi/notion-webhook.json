              "schema": {
                "properties": {
                  "body": {
                    "properties": {
                      "properties": {
                        "type": "object"
                      },
                      "title": {
                        "items": {
                          "type": "object"
                        },
                        "type": "array"
                      }
                    },
                    "type": "object"
                  },
                  "database_id": {
                    "type": "string"
                  }
                },
                "required": [
                  "database_id",
                  "body"
                ],
                "type": "object"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Updated database"
          },
